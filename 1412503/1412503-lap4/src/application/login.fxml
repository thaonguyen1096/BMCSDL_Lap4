<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="322.0" prefWidth="475.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" 
			fx:controller = "application.loginController">
   <children>
      <Label layoutX="61.0" layoutY="94.0" text="Username">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="138.0" text="Password">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="textUsername" layoutX="175.0" layoutY="82.0" prefHeight="37.0" prefWidth="232.0">
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </TextField>
      <PasswordField fx:id="textPassword" layoutX="175.0" layoutY="131.0" prefHeight="37.0" prefWidth="232.0" />
      <Button fx:id="btnLogin" onAction = "#showLogin" layoutX="175.0" layoutY="239.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="87.0" text="Login" >
         <font>
            <Font name="Times New Roman" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" onAction = "#showCancel" layoutX="320.0" layoutY="239.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="87.0" text="Cancel">
         <font>
            <Font name="Times New Roman" size="20.0"> </Font>
         </font>
      </Button>
   </children>
</AnchorPane>
